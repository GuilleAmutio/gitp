{
  "name": "gitp",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "gitp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "link": "npm link",
    "start": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "prepare": "chmod +x dist/index.js",
    "cleanup": "rm -f ~/.gitp"
  },
  "description": "",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.1.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.9.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "git",
    "profile",
    "cli"
  ],
  "author": "Guilermo Amutio",
  "license": "MIT"
}
